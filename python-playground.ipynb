    "#Curiosit√† sull'andamento della funzione fib\n",
    "from math import sqrt\n",
    "\n",
    "phi = (1+sqrt(5))/2\n",
    "F0 = fib(1)\n",
    "print(\"n\\tRapporto F_n/F_n-1={}\\tScarto con phi\")\n",
    "for n in range(2,51):\n",
    "    F1 = fib(n)\n",
    "    R = F1/F0\n",
    "    F0 = F1\n",
    "    print(\"{0}\\t{1:.10f}\\t\\t{2:.3}\".format(n,R,(R-phi)/phi))"
   ]
