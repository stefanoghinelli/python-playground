    "#Funzioni\n",
    "#Calcola i numeri di Fibonacci fino all'n-esimo\n",
    "def fib(n):\n",
    "    f0 = 0\n",
    "    f1 = 1\n",
    "    for _ in range(n-1):\n",
    "      f0, f1 = f1, f0+f1\n",
    "    return f1\n",
    "\n",
    "fib(10)"
   ]
  },
