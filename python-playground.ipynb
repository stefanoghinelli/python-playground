    "#Map-Reduce\n",
    "from functools import reduce\n",
    "\n",
    "def sum_of_squares_MR(n):\n",
    "    s=map(lambda i: i*i,range(1,n+1))\n",
    "    return reduce(lambda x,y:x+y,s)\n",
    "\n",
    "sum_of_squares_MR(50)"
